{
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "rules": {
    "max-line-length": {
      "options": [120]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-implicit-dependencies": false
  },
  "jsRules": {
    "max-line-length": {
      "options": [120]
    }
  }
}
